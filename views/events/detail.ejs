                <div class="panel price">
                <div class="panel-heading">
                  <div class="row">

                     <div class="col-lg-12">
                         
                          <div class="col-lg-12">
                              <h2><a href="">Description :</a></h2>
                              <p><%= eventDescription %></p>
                              <h4>Humain Resource Needed</h4>
                               <table class="table">
                                    <thead>
                                    
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td style="width:80%;"><%= eventDescription %> person needed </td>
                                        <td> <button type="submit" class="btn btn-success btn-lg">Join</button></td>
                                    </tr>
                                    
                                    </tbody>
                                </table>
                                            <h4>Tools Needed</h4>                 
                                    <table class="table">
                                    <thead>
                                    <tr>
                                        <th>Tool</th>
                                        <th>Number</th>
                                        <th>Action</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
