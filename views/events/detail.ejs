                <div id="mapid" class="panel-heading">
                    <div class="maping-details"></div>
                </div>
                
                <ul class="list-group list-group-flush">
                    <li class="list-group-item"><br><p><b>Publisher :</b>  <%=name %></p><p><b>Start Date : </b>b><%=s_date %></p><p><b>End Date :</b> <%=end_date %></p>
                 
                   
                    <table style="width:100%;" class="table table-bordered">
  
    <tbody>
      <tr>
        <td style="width:50%;text-align:center;"><%=participent %><br> Personne</td>
        <td style="width:50%;text-align:center;"><%=needs %><br> Tools</td>
      </tr>
    </tbody>
  </table>
                        <p><b>Address :</b> <%= adress %></p>
                        
                      </li>
                   
                </ul>
            </div>
            </div>
            
                <div class="col-lg-8">
